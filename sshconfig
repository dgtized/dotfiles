Host sandbox
     HostName sandbox.adjudica.net
     RemoteForward 1983 localhost:8080

Host vm1
     HostName vm1.adjudica.net
     LocalForward 7071 localhost:7071

Host vm2
     HostName vm2.adjudica.net
     # jmx and insight respectively
     LocalForward 6969 localhost:6969 
     LocalForward 8081 localhost:8081

Host vm3
     HostName vm3.adjudica.net
     # LocalForward 7071 localhost:7071

Host nocturnal
     HostName 24.107.184.225
     #HostName nocturnal.uk.to

# proxy host1 through sandbox
Host host1
     ProxyCommand ssh -q sandbox nc -q0 host1.adjudica.hostirian.com 22
     User root

Host *
     ForwardAgent yes
     Compression yes
     ForwardX11 yes
     User clgc
     # share a single ssh connection
     ControlPath ~/.ssh/control/%r@%h:%p
     ControlMaster auto
     ControlPersist 60
